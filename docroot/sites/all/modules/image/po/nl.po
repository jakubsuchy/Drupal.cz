msgstr ""

#: image.module:59
msgid "Default image path"
msgstr "Standaard pad voor afbeeldingen"

#: image.module:59
msgid "Subdirectory in the directory \"%dir\" where pictures will be stored."
msgstr "Submap van de map \"%dir\" waar afbeeldingen bewaard zullen worden."

