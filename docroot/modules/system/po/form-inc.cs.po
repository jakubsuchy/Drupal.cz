#: includes/form.inc:395
msgid ""
"Validation error, please try again. If this error persists, please contact "
"the site administrator."
msgstr "Zjištěna chyba validace. Pokud tento problém přetrvává, kontaktujte administrátora webu."

#: includes/form.inc:536
msgid "!name field is required."
msgstr "Pole !field je povinné."

#: includes/form.inc:541
msgid ""
"!name cannot be longer than %max characters but is currently %length "
"characters long."
msgstr "Pole !name nemůže být delší než %max znaků, nyní je %length znaků dlouhé."

#: includes/form.inc:558
msgid "Illegal choice %choice in !name element."
msgstr "Neplatná volba %choice v elementu !name."

#: includes/form.inc:1120
msgid "Confirm password"
msgstr "Potvrzení hesla"

#: includes/form.inc:1142
msgid "The specified passwords do not match."
msgstr "Zadaná hesla se neshodují."

#: includes/form.inc:1146
msgid "Password field is required."
msgstr "Pole pro zadání hesla je povinné."

#: includes/form.inc:1224
msgid "The specified date is invalid."
msgstr "Zadané datum není platné."

#: includes/form.inc:1540
msgid "This field is required."
msgstr "Toto pole je povinné."

#: includes/form.inc:1545;1548
msgid "!title: !required"
msgstr "!title: !required"

